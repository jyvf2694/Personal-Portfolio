{
  "name": "jonathan-velandia-portafolio-personal",
  "version": "1.0.0",
  "description": "Jonathan Velandia | Portafolio Personal",
  "main": "custom.js",
  "scripts": {
    "sass": "sass --style=compressed dist/scss:dist/css",
    "prefix": "npm run sass && postcss dist/css/**/*.css --use=autoprefixer --dir dist/css --map",
    "build": "onchange \"dist/scss/**/*.scss\" -- npm run prefix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "author": "velandiajonathan02@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "onchange": "^7.1.0",
    "postcss": "^8.4.44",
    "postcss-cli": "^11.0.0",
    "sass": "^1.77.8"
  }
}
